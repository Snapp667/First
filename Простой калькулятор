a = float(input())
b = float(input())
c = input()
if not b and ( c=='mod' or c=='/' or c=='div'):
    print("Деление на 0!")
elif c == '-':
    print(a-b)
elif c == '/':
    print(a/b)
elif c == '+':
    print(a+b)
elif c == '*':
    print(a*b)
elif c == 'mod':
    print(a % b)
elif c == 'pow':
    print(a**b)
elif c == 'div':
    print(a//b)
